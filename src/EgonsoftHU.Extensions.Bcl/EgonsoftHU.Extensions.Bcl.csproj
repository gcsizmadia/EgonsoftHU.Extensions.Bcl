<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  <PropertyGroup>
    <Authors>Gabor Csizmadia</Authors>
    <Owners>Egonsoft.HU</Owners>
    <Copyright>Copyright © 2022 Gabor Csizmadia</Copyright>
    <Title>Egonsoft.HU BCL Extensions</Title>
    <Description>Extension methods for BCL types (String, Type, etc.)</Description>
    <Summary>Extension methods for BCL types</Summary>
    <PackageId>EgonsoftHU.Extensions.Bcl</PackageId>
    <VersionPrefix>1.0.0</VersionPrefix>
    <PackageTags>$(PackageTags) extensions extension bcl object string type assembly parameterinfo</PackageTags>
    <PackageProjectUrl>https://github.com/gcsizmadia/EgonsoftHU.Extensions.Bcl</PackageProjectUrl>
    <PackageReleaseNotes>Please visit https://github.com/gcsizmadia/EgonsoftHU.Extensions.Bcl/releases to view the release notes.</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <MinClientVersion>2.8</MinClientVersion>
    <DevelopmentDependency>false</DevelopmentDependency>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/gcsizmadia/EgonsoftHU.Extensions.Bcl</RepositoryUrl>
  </PropertyGroup>
</Project>
